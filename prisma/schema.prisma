// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite.db"
}

model Collection {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  name        String
  url         String
  lastRefresh DateTime?
  favIcon     String?
}
